@model UNFHackAThon.Models.ViewModels.IndexViewModel


<br />

<br />
<br />

<div class="backgroundWhite container">

    <ul id="menu-filters" class="menu-filter-list list-inline text-center">
        <li class="filter active btn btn-secondary ml-1 mr-1">Show All</li>

        @foreach (var item in Model.Competition)
        {
            <li class="filter ml-1 mr-1">@item.Name</li>
        }
    </ul>


    @foreach (var competition in Model.Competition)
    {
        <div class="row" id="menu-wrapper">
            <partial name="_ThumbnailAreaPartial" model="@Model.CompetitionItem.Where(u=>u.Competition.Name.Equals(competition.Name))" />
        </div>
    }
</div>

